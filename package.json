{
  "author": "Espen Norderud",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "contributors": [
    "Christian Andreas Westgaard <cwe@enonic.com>"
  ],
  "dependencies": {
    "@enonic/js-utils": "^0.46.0",
    "node-fetch": "^3.3.1",
    "xxhashjs": "^0.2.2"
  },
  "description": "XP serverside runtime, handles the bulk of the React4xp work: server-side rendering, and handling and serving of client-side components and dependency chunks to the client, including the React4xp front-end rendering wrapper itself.\"",
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.21.0",
    "@enonic-types/global": "^7.12.0",
    "@enonic-types/lib-node": "^7.12.0",
    "@enonic/react4xp": "^3.1.2",
    "@mrhenry/core-web": "^1.1.0",
    "@rollup/plugin-alias": "^4.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-sucrase": "^5.0.1",
    "@rollup/plugin-typescript": "11.0.0",
    "@swc/core": "^1.3.40",
    "@swc/helpers": "^0.4.14",
    "@types/core-js": "^2.5.5",
    "@types/react": "^18",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.2",
    "babel-polyfill": "^6.26.0",
    "core-js-pure": "^3.29.1",
    "esbuild-plugin-manifest": "^0.5.0",
    "eslint": "^8.36.0",
    "eslint-formatter-pretty": "^4.1.0",
    "glob": "^9.2.1",
    "lodash": "^4.17.21",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "react-dom": "^18",
    "rollup": "^3.19.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-output-manifest": "^2.0.0",
    "rollup-plugin-swc3": "^0.8.0",
    "rollup-plugin-ts": "^3.2.0",
    "text-encoding": "^0.7.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5"
  },
  "main": "src/main/resources/lib/enonic/react4xp/index.es6",
  "name": "lib-react4xp",
  "keywords": [
    "React4xp",
    "Enonic",
    "XP",
    "React"
  ],
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "clean": "rimraf build",
    "cleanAll": "rimraf build node_modules && ./gradlew clean --refresh-dependencies --info",
    "dev": "npm run clean && npm run local:watch",
    "lint": "npx eslint --format=pretty src/main/resources/**/*.ts",
    "local": "./gradlew publishToMavenLocal -P development --info",
    "local:watch": "./gradlew publishToMavenLocal -P development --info -t",
    "install:nsi": "nsi",
    "reinstall": "npm run clean && npm run local",
    "rollup": "npm-run-all --parallel rollup:client rollup:executor rollup:nashornPolyfills rollup:nodePolyfills rollup:server",
    "rollup:client": "npx rollup --config rollup.config.client.mjs",
    "rollup:executor": "npx rollup --config rollup.config.executor.mjs",
    "rollup:nashornPolyfills": "npx rollup --config rollup.config.nashornPolyfills.mjs",
    "rollup:nodePolyfills": "npx rollup --config rollup.config.nodePolyfills.mjs",
    "rollup:server": "npx rollup --config rollup.config.server.mjs src/main/resources/lib/enonic/react4xp/index.ts src/main/resources/services/react4xp/react4xp.ts src/main/resources/services/react4xp-dependencies/react4xp-dependencies.ts",
    "test": "NODE_PATH=./ node_modules/mocha/bin/mocha --compilers js:@babel/register --require babel-polyfill --require src/test/test_helper.js \"src/test/**/*@(.js)\"",
    "tsc": "npm-run-all --parallel tsc:*",
    "tsc:assets": "npx tsc -p src/main/resources/assets/tsconfig.json --noEmit",
    "tsc:resources": "npx tsc --noEmit",
    "tsup": "npm-run-all --parallel tsup:*",
    "tsup:client": "npx tsup --entry.client src/main/resources/assets/react4xp/client.ts",
    "tsup:executor": "npx tsup --entry.executor src/main/resources/assets/react4xp/executor.ts",
    "tsup:server": "npx tsup -d build/resources/main"
  }
}
