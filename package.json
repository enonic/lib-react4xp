{
  "name": "lib-react4xp",
  "version": "0.5.1",
  "description": "XP serverside runtime, handles the bulk of the React4xp work: server-side rendering, and handling and serving of client-side components and dependency chunks to the client, including the React4xp front-end rendering wrapper itself.\"",
  "main": "src/main/resources/lib/enonic/react4xp/index.es6",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "NODE_PATH=./ node_modules/mocha/bin/mocha --compilers js:babel-core/register --require babel-polyfill --require src/test/test_helper.js \"src/test/**/*@(.js)\"",
    "clean": "./gradlew clean --refresh-dependencies --info",
    "local": "./gradlew publishToMavenLocal --info",
    "reinstall": "npm run clean && npm run local",
    "watch": "./gradlew publishToMavenLocal --info -t",
    "dev": "npm run reinstall && npm run watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enonic/lib-react4xp.git"
  },
  "keywords": [
    "React4xp"
  ],
  "author": "Espen Norderud",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/enonic/lib-react4xp/issues"
  },
  "homepage": "https://github.com/enonic/lib-react4xp#readme",
  "dependencies": {
    "node-fetch": "^1.7.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "lodash": "^4.17.13",
    "mocha": "^5.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react4xp-build-components": "^0.6.0",
    "react4xp-buildconstants": "^0.8.0",
    "react4xp-runtime-client": "^0.3.2",
    "react4xp-runtime-nashornpolyfills": "^0.3.6",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  }
}
