{
  "author": "Espen Norderud",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "contributors": [
    "Christian Westgaard <cwe@enonic.com>"
  ],
  "dependencies": {
    "@enonic/js-utils": "^0.36.0",
    "node-fetch": "^3.2.4",
    "xxhashjs": "^0.2.2"
  },
  "description": "XP serverside runtime, handles the bulk of the React4xp work: server-side rendering, and handling and serving of client-side components and dependency chunks to the client, including the React4xp front-end rendering wrapper itself.\"",
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.0",
    "@babel/preset-env": "^7.17.10",
    "@babel/register": "^7.17.7",
    "@enonic/react4xp": "^3.0.17",
    "@mrhenry/core-web": "^0.7.1",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/react": "^18.0.9",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.5",
    "babel-polyfill": "^6.26.0",
    "core-js-pure": "^3.22.6",
    "eslint": "^8.15.0",
    "lodash": "^4.17.21",
    "mocha": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "react-dom": "*",
    "rollup": "^2.74.1",
    "rollup-plugin-output-manifest": "^2.0.0",
    "rollup-plugin-polyfill": "^4.0.0",
    "text-encoding": "^0.7.0",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  },
  "main": "src/main/resources/lib/enonic/react4xp/index.es6",
  "name": "lib-react4xp",
  "keywords": [
    "React4xp",
    "Enonic",
    "XP",
    "React"
  ],
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "clean": "rimraf build",
    "cleanAll": "rimraf build node_modules && ./gradlew clean --refresh-dependencies --info",
    "dev": "npm run clean && npm run local:watch",
    "local": "./gradlew publishToMavenLocal -P development --info",
    "local:watch": "./gradlew publishToMavenLocal -P development --info -t",
    "install:nsi": "nsi",
    "reinstall": "npm run clean && npm run local",
    "rollup": "npm-run-all --parallel rollup:client rollup:executor rollup:nashornPolyfills rollup:nodePolyfills rollup:server",
    "rollup:client": "npx rollup --config rollup.config.client.js",
    "rollup:executor": "npx rollup --config rollup.config.executor.js",
    "rollup:nashornPolyfills": "npx rollup --config rollup.config.nashornPolyfills.js",
    "rollup:nodePolyfills": "npx rollup --config rollup.config.nodePolyfills.js",
    "rollup:server": "npx rollup --config rollup.config.server.js src/main/resources/lib/enonic/react4xp/index.ts src/main/resources/services/react4xp/react4xp.ts src/main/resources/services/react4xp-dependencies/react4xp-dependencies.ts",
    "test": "NODE_PATH=./ node_modules/mocha/bin/mocha --compilers js:@babel/register --require babel-polyfill --require src/test/test_helper.js \"src/test/**/*@(.js)\"",
    "tsc": "npx tsc"
  }
}
