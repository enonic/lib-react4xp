{
  "author": "Espen Norderud",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "contributors": [
    "Christian Westgaard <cwe@enonic.com>"
  ],
  "dependencies": {
    "@enonic/js-utils": "^0.33.3",
    "node-fetch": "^3.2.3",
    "xxhashjs": "^0.2.2"
  },
  "description": "XP serverside runtime, handles the bulk of the React4xp work: server-side rendering, and handling and serving of client-side components and dependency chunks to the client, including the React4xp front-end rendering wrapper itself.\"",
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.7",
    "@enonic/react4xp": "^3.0.16",
    "@mrhenry/core-web": "^0.7.0",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/react": "^18.0.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.4",
    "babel-polyfill": "^6.26.0",
    "core-js-pure": "^3.22.2",
    "eslint": "^8.13.0",
    "lodash": "^4.17.21",
    "mocha": "^9.2.2",
    "npm-run-all": "^4.1.5",
    "react-dom": "*",
    "rollup": "^2.70.2",
    "rollup-plugin-output-manifest": "^2.0.0",
    "rollup-plugin-polyfill": "^4.0.0",
    "text-encoding": "^0.7.0",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "main": "src/main/resources/lib/enonic/react4xp/index.es6",
  "name": "lib-react4xp",
  "keywords": [
    "React4xp",
    "Enonic",
    "XP",
    "React"
  ],
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "clean": "rimraf build",
    "cleanAll": "rimraf build node_modules && ./gradlew clean --refresh-dependencies --info",
    "dev": "npm run clean && npm run local:watch",
    "local": "./gradlew publishToMavenLocal -P development --info",
    "local:watch": "./gradlew publishToMavenLocal -P development --info -t",
    "install:nsi": "nsi",
    "reinstall": "npm run clean && npm run local",
    "rollup": "npm-run-all --parallel rollup:client rollup:executor rollup:nashornPolyfills rollup:server",
    "rollup:client": "npx rollup --config rollup.config.client.js",
    "rollup:executor": "npx rollup --config rollup.config.executor.js",
    "rollup:nashornPolyfills": "npx rollup --config rollup.config.nashornPolyfills.js --no-makeAbsoluteExternalsRelative",
    "rollup:server": "npx rollup --config rollup.config.server.js --no-makeAbsoluteExternalsRelative $(find src/main/resources ! -path 'src/main/resources/assets*' -a ! -path 'src/main/resources/lib/enonic/react4xp/nashornPolyfills.ts' -a -name '*.ts' -a ! -name '*.d.ts')",
    "test": "NODE_PATH=./ node_modules/mocha/bin/mocha --compilers js:@babel/register --require babel-polyfill --require src/test/test_helper.js \"src/test/**/*@(.js)\"",
    "tsc": "npx tsc"
  }
}
